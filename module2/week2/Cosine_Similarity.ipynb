   "source": [
    "def compute_cosine(v1, v2):\n",
    "    x_dot_y = v1.dot(v2)\n",
    "    x_mul_y = np.linalg.norm(v1) * np.linalg.norm(v2)\n",
    "    cos_sim = x_dot_y / x_mul_y\n",
    "    return cos_sim\n",
    "\n",
    "v1 = np.array([1,2,3,4])\n",
    "v2 = np.array([1,0,3,0])\n",
    "compute_cosine(v1, v2)"
   ]
  }
